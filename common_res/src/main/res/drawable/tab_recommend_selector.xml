<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 不可用时 -->
    <item android:drawable="@drawable/icon_recommand_p" android:state_enabled="false" />
    <!-- 按压时 -->
    <item android:drawable="@drawable/icon_recommand_p" android:state_pressed="true" />
    <!-- 被选中时 -->
    <item android:drawable="@drawable/icon_recommand_p" android:state_selected="true" />
    <!-- 被激活时 -->
    <item android:drawable="@drawable/icon_recommand_p" android:state_activated="true" />
    <!-- 默认时 -->
    <item android:drawable="@drawable/icon_recommand_n" />

</selector>